{
  "name": "charm-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "frontend": "react-app-rewired start",
    "backend": "nodemon server.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "clean": "rm -rf build && mkdir build",
    "build": "react-app-rewired build && npm run build-backend && npm run copy",
    "start": "node build/server.js",
    "heroku-postbuild": "npm i react-app-rewired",
    "build-backend": "babel ./backend -d build/backend",
    "copy": "cp server.js build/",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "10.10.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "mobx": "^5.1.0",
    "mobx-devtools-mst": "^0.9.18",
    "mobx-react": "^5.2.8",
    "moment": "^2.22.2",
    "mongoose": "^5.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-datepicker": "^1.7.0",
    "react-dates": "^18.1.1",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-with-direction": "^1.3.0",
    "recharts": "^1.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.0.1",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.4",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.6.2"
  }
}
