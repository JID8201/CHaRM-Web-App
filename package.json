{
  "name": "charm-web-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "backend": "nodemon server.js --exec babel-node --presets=@babel/env",
    "build": "npm run clean && npm run build-frontend && npm run build-backend && npm run copy",
    "build-backend": "babel --presets @babel/env ./backend -d build/backend",
    "build-frontend": "webpack -p --config webpack.config.prod.js",
    "clean": "rimraf build && mkdir build",
    "copy": "copyfiles server.js build/",
    "debug": "nodemon server.js --inspect --exec babel-node --presets=@babel/env",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "eject": "react-scripts eject",
    "frontend": "webpack-dev-server --hot",
    "postinstall": "cross-env NODE_ENV=production npm run build",
    "start": "node build/server.js",
    "test": "react-app-rewired test --env=jsdom"
  },
  "engines": {
    "node": "10.10.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "fast-csv": "^2.4.1",
    "helmet": "^3.15.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.4.0",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.8",
    "moment": "^2.22.2",
    "mongoose": "^5.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dates": "^18.1.1",
    "react-dom": "^16.6.0",
    "react-heatmap-jsx": "^1.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-with-direction": "^1.3.0",
    "recharts": "^1.3.4",
    "superagent": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mobx-react-devtools": "^6.0.3",
    "nodemon": "^1.18.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.3.12",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "proxy": "http://localhost:3001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
