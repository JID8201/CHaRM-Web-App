{
  "name": "charm-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "frontend": "webpack-dev-server --hot",
    "backend": "nodemon server.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && npm run build-frontend && npm run build-backend && npm run copy",
    "start": "node build/server.js",
    "build-frontend": "webpack -p --config webpack.config.prod.js",
    "build-backend": "babel ./backend -d build/backend",
    "copy": "cp server.js build/",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "NODE_ENV=production npm run build"
  },
  "engines": {
    "node": "10.10.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "fast-csv": "^2.4.1",
    "isomorphic-fetch": "^2.2.1",
    "mobx": "^5.1.0",
    "mobx-devtools-mst": "^0.9.18",
    "mobx-react": "^5.2.8",
    "moment": "^2.22.2",
    "mongoose": "^5.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dates": "^18.1.1",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-with-direction": "^1.3.0",
    "recharts": "^1.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "nodemon": "^1.18.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.3.12",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "proxy": "http://localhost:3001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
